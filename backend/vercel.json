{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/index.js",
      "methods": ["GET", "POST", "DELETE", "PUT", "OPTIONS"]
    }
  ],
  "installCommand": "npm install --legacy-peer-deps"
}
